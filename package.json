{
  "name": "react-ssr",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.15.0"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-flow": "7.17.12",
    "@babel/preset-react": "7.17.12",
    "babel-loader": "8.2.5",
    "compression": "1.7.4",
    "concurrently": "5.3.0",
    "core-js": "3.23.2",
    "cross-env": "7.0.3",
    "express": "4.18.1",
    "nodemon": "2.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "resolve": "1.22.1",
    "regenerator-runtime": "0.13.9",
    "regenerator-transform": "0.14.5",
    "rimraf": "^3.0.2",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run bundler:dev\"",
    "start:prod": "concurrently \"npm run server:prod\" \"npm run bundler:prod\"",
    "server:dev": "cross-env NODE_ENV=development nodemon -- server/server.js",
    "server:prod": "cross-env NODE_ENV=production nodemon -- server/server.js",
    "bundler:dev": "cross-env NODE_ENV=development nodemon -- scripts/build.js",
    "bundler:prod": "cross-env NODE_ENV=production nodemon -- scripts/build.js"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ]
  }
}
